        </Field>
        <Field Name="Qty">
          <DataField>Qty</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="UOM">
          <DataField>UOM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WeekNo">
          <DataField>WeekNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
