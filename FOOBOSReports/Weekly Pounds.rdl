  <DataSets>
    <DataSet Name="OrdersSuppliers">
      <Query>
        <DataSourceName>FOOBOSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@orderDate">
            <Value>=Parameters!orderDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ord.OrderNum
	, ord.OrderDate
	, sup.Supplier
	, coalesce(prod.Name, prodcat.Name) Product
	, ordln.pkid OrderlineId
	, ordln.LineNum
	, ordln.Qty
	, coalesce(uom1.Name, uom2.Name) UOM
	, DATEPART(week, ord.OrderDate) WeekNo
from [Order] ord
	left outer join Supplier sup on ord.SupplierID = sup.pkid
	left outer join OrderLine ordln on ord.pkid = ordln.OrderID
	left outer join Product prod on ordln.ProductID = prod.pkid
	left outer join ProductCategory prodcat on ordln.ProductCategoryID = prodcat.pkid
	left outer join UOM uom1 on uom1.pkid = prod.UOMID
	left outer join uom uom2 on uom2.pkid = prodcat.UOMID
where 1=1
	and ord.OrderDate &lt;= coalesce(@orderDate, '12-31-2099')
	and LineNum is not null</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OrderNum">
          <DataField>OrderNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Supplier">
